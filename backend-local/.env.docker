# Database - automatically configured by docker-compose
DATABASE_URL=postgresql://wildraft:wildraft_secret@postgres:5432/wildraft?schema=public

# Server
PORT=3000
HOST=0.0.0.0

# S3/Object Storage - fill in your actual values
AWS_REGION=default
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_S3_BUCKET=wildraft
AWS_ENDPOINT=https://eu2.contabostorage.com

# Environment
NODE_ENV=production
